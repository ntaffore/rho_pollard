/* algoithme de floyd poyur comprendre la collision rapide a(i) = a(2i) */

/* fonction f */


func(E,W1,W2,P,Q,a,b) = {

		c = Mod(a+b,3);
		if ( c == 0, W2 = elladd(E,W2,Q); b=b+1;);
		if ( c == 1, W2 = elladd(E,W2,W2); a=a+a;b=b+b;);
		if ( c == 2, W2 = elladd(E,W2,P); a=a+1;);
		return([W1,W2,P,Q,a,b]);

}


/* foncion qui ecrit les cycle */

